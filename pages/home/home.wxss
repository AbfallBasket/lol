page {
  background-color: #566a78;
}
page .hero-list {
  display: flex;
  flex-wrap: wrap;
}
page .hero-list .hero-list-box {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 33.333%;
  box-sizing: border-box;
  padding-bottom: 4px;
  padding-top: 10px;
}
page .hero-list .hero-list-box image {
  width: 88px;
  height: 88px;
}
